<!DOCTYPE html>
<html ng-app="twitterApp">
<head>
    <title>twitter</title>
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <link rel="stylesheet" href="/assets/vendor/bootstrap/dist/css/bootstrap.css"/>

    <script src="/assets/vendor/angular/angular.js"></script>
    <script src="/assets/vendor/angular-resource/angular-resource.js"></script>
    <script src="/assets/vendor/jquery/dist/jquery.js"></script>
    <script src="/assets/vendor/bootstrap/dist/js/bootstrap.js"></script>
    <script src="/assets/js/controllers.js"></script>
</head>

<body ng-controller="TweetController">

<div class="container-fluid">

    <ul>
        <div ng-repeat="tweet in tweets">
            <span>@{{tweet.user.screen_name}}</span>
            <p>{{tweet.text}}</p>
        </div>
    </ul>

</div>

</body>
</html>